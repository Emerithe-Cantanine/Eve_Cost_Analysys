BEGIN TRANSACTION;

INSERT INTO MarketOrdersAll
(
typeID, volume, volumeRemaining, solarSystemID, range, price, orderID, stationID,
dateIssued, buy_true_sell_false, duration, distanceHome, contiguousHighsec, tradeHub,
distanceAmarr, distanceJita, distanceHek, distanceDodixie
)

SELECT typeID, volume, volumeRemaining, solarSystemID, range, price, orderID, stationID,
dateIssued, buy_true_sell_false, duration, distanceHome, contiguousHighsec, tradeHub,
distanceAmarr, distanceJita, distanceHek, distanceDodixie

FROM PurchaseOrders
WHERE orderID = OrderID# and buy_true_sell_false = 0;

DELETE FROM PurchaseOrders
WHERE orderID = OrderID# and buy_true_sell_false = 0;

COMMIT TRANSACTION;